<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mem.wit.study.myBatis.LecCateMapper">
	<resultMap id="LecCateMap" type="mem.wit.study.codes.LecCate">
	</resultMap>
  <select id="selectMember_basic" resultType="com.wit.member.Member">
    $value$
  </select>
  <select id="select" resultType="mem.wit.study.codes.LecCate">
    SELECT * FROM cdLecCate
		WHERE lcCode = #{lcCode}
  </select>
  <select id="selectAll" resultMap="mem.wit.study.myBatis.LecCateMapper.LecCateMap">
    SELECT * FROM cdLecCate
  </select>
</mapper>