	<insert id="insertSE" parameterClass="mem.wit.openex.OsetExamDTO">
		INSERT INTO O_SetExam
		VALUES
		(SEQ_exNo.NEXTVAL,#MID#,#EXTIMELIMIT#,#DIF_NO#,#EX_QUESTIONSCOUNT#,#EX_TITLE#,
		#EX_EXPLAIN#,0,0,#EX_RECOMMENDLECTURE#,#EX_REFERENCE#,SYSDATE,0,#MD_NO#,#EX_MNNAME#)
	</insert>

	<insert id="insertQUE" parameterClass="mem.wit.openex.OsetExamQueDTO">
		<selectKey resultClass="int" keyProperty="EX_NO">
			SELECT MAX(EX_NO)
			AS EX_NO FROM O_SETEXAM
		</selectKey>
		INSERT INTO O_Questions
		VALUES
		(#EX_NO#,SEQ_que_QueNo.NEXTVAL,#QUE_USERSUBJECT#,#QUE_CONTENT#,#QUE_SCORE#,
		#QUE_EXAMPLE1#,#QUE_EXAMPLE2#,#QUE_EXAMPLE3#,#QUE_EXAMPLE4#,#QUE_EXAMPLE5#,#QUE_RIGHTANSWER#,#QUE_QNO#)
	</insert>

	<update id="updateQUE" parameterClass="mem.wit.openex.OsetExamQueDTO">
		UPDATE O_QUESTIONS
		SET
		QUE_USERSUBJECT=#QUE_USERSUBJECT#,
		QUE_CONTENT=#QUE_CONTENT#,QUE_SCORE
		= #QUE_SCORE#,
		QUE_EXAMPLE1 = #QUE_EXAMPLE1#, QUE_EXAMPLE2
		=#QUE_EXAMPLE2#, QUE_EXAMPLE3 =#QUE_EXAMPLE3#,
		QUE_EXAMPLE4
		=#QUE_EXAMPLE5#, QUE_EXAMPLE5 =#QUE_EXAMPLE5#,
		QUE_RIGHTANSWER=#QUE_RIGHTANSWER#
		WHERE QUE_QNO=#QUE_QNO# AND EX_NO =
		#EX_NO#
	</update>

