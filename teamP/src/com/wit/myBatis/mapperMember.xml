<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wit.myBatis.MemberMapper">
  <select id="selectMember_basic" resultType="com.wit.member.Member">
    $value$
  </select>
  <select id="login" resultType="com.wit.member.Member">
    SELECT * FROM WitMember
		WHERE mId = #{mId}
  </select>
  <select id="getPower" resultType="com.wit.member.Power">
    SELECT * FROM power
		WHERE mId = #{mId}
  </select>
</mapper>